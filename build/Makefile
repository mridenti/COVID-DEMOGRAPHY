all: $(PROJECTS)

Libraries:
ifneq (,$(Libraries_config))
	@echo "==== Building Libraries ($(Libraries_config)) ===="
	@${MAKE} --no-print-directory -C . -f Libraries.make config=$(Libraries_config)
endif

spatial_covid0d_estrat: Libraries
ifneq (,$(spatial_covid0d_estrat_config))
	@echo "==== Building spatial_covid0d_estrat ($(spatial_covid0d_estrat_config)) ===="
	@${MAKE} --no-print-directory -C . -f spatial_covid0d_estrat.make config=$(spatial_covid0d_estrat_config)
endif

csv_to_input: Libraries
ifneq (,$(csv_to_input_config))
	@echo "==== Building csv_to_input ($(csv_to_input_config)) ===="
	@${MAKE} --no-print-directory -C . -f csv_to_input.make config=$(csv_to_input_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Libraries.make clean
